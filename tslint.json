{
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [true, "always"],
		"triple-equals": true,
		"trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
		"only-arrow-functions": [true, "allow-declarations"]
	}
}